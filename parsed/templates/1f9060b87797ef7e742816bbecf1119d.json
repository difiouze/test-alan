{
  "components/amp/products/videos": "<section expanded class=\"productAccordion\" data-video-gallery>\r\n    <div class=\"productAccordion-title\">\r\n        {{lang 'products.videos.header'}}\r\n    </div>\r\n    <div class=\"productAccordion-content\">\r\n        <amp-carousel width=\"640\"\r\n            height=\"490\"\r\n            layout=\"responsive\"\r\n            type=\"slides\">\r\n            {{#each this.list}}\r\n                <div>\r\n                    <div>\r\n                        <amp-youtube\r\n                            data-videoid=\"{{id}}\"\r\n                            layout=\"responsive\"\r\n                            width=\"640\"\r\n                            height=\"390\">\r\n                        </amp-youtube>\r\n                    </div>\r\n                    <div class=\"video-body\">\r\n                        <div class=\"video-title\">{{title_long}}</div>\r\n                        <p class=\"video-description\">{{description_short}}</p>\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </amp-carousel>\r\n    </div>\r\n</section>\r\n"
}