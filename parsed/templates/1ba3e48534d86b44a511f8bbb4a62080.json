{
  "components/amp/common/footer": "<footer class=\"amp-footer\" role=\"contentinfo\">\r\n\r\n    {{#if theme_settings.footer_show_aboutus}}\r\n    <article class=\"amp-footer-col amp-footer-col--about\" data-section-type=\"storeInfo\">\r\n        <div class=\"amp-footer-heading\">{{> components/amp/common/store-logo}}</div>\r\n        {{#if theme_settings.footer_show_contact}}\r\n            <address>{{nl2br settings.address}}</address>\r\n            {{#if settings.phone_number}}\r\n                <strong>{{lang 'footer.call_us' phone_number=settings.phone_number}}</strong>\r\n            {{/if}}\r\n        {{/if}}\r\n    </article>\r\n    {{/if}}\r\n\r\n    <article class=\"amp-footer-col amp-footer-col--pages\" data-section-type=\"footer-webPages\">\r\n        <div class=\"amp-footer-heading\">{{lang 'footer.navigate'}}</div>\r\n        <ul class=\"amp-footer-list\">\r\n            {{#each pages}}\r\n            <li>\r\n                <a href=\"{{url}}\">{{name}}</a>\r\n            </li>\r\n            {{/each}}\r\n            <li>\r\n                <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\r\n            </li>\r\n        </ul>\r\n    </article>\r\n\r\n    <article class=\"amp-footer-col amp-footer-col--categories\" data-section-type=\"footer-categories\">\r\n        <div class=\"amp-footer-heading\">{{lang 'footer.categories'}}</div>\r\n        <ul class=\"amp-footer-list\">\r\n            {{#each categories}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </article>\r\n\r\n    <article class=\"amp-footer-col amp-footer-col--brands\" data-section-type=\"footer-brands\">\r\n        <div class=\"amp-footer-heading\">{{lang 'footer.brands'}}</div>\r\n        <ul class=\"amp-footer-list\">\r\n            {{#each shop_by_brand}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n            {{/each}}\r\n            <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\r\n        </ul>\r\n    </article>\r\n\r\n    <article class=\"amp-footer-copyright\">\r\n        {{#if theme_settings.show_powered_by}}\r\n            <p>{{lang 'footer.powered_by'}} <a href=\"http://bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\r\n        {{/if}}\r\n        {{#if theme_settings.show_copyright_footer}}\r\n            <p class=\"powered-by\">&copy; {{settings.store_name}} </p>\r\n        {{/if}}\r\n    </article>\r\n</footer>\r\n",
  "components/amp/common/store-logo": "<a href=\"{{urls.home}}\" class=\"amp-header-link\">\r\n    {{#if settings.store_logo.image}}\r\n        <amp-img src=\"{{getImage settings.store_logo.image 'logo_size'}}\"\r\n            {{#if theme_settings.logo_size '===' 'original' }}\r\n                width=\"100\"\r\n                height=\"50\"\r\n            {{else}}\r\n                width=\"{{first (split theme_settings.logo_size 'x')}}\"\r\n                height=\"{{last (split theme_settings.logo_size 'x')}}\"\r\n            {{/if}}\r\n            alt=\"{{settings.store_logo.title}}\">\r\n        </amp-img>\r\n    {{else}}\r\n        <span class=\"amp-header-logo-text\">{{settings.store_logo.title}}</span>\r\n    {{/if}}\r\n</a>\r\n"
}