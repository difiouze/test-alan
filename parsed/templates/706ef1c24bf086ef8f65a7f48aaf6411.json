{
  "components/common/quick-search": "<div class=\"emthemesModez-quickSearch{{#if style}} emthemesModez-quickSearch--{{style}}{{else}}{{#if theme_settings.quickSearch-desktop-style '!==' 'box'}} emthemesModez-quickSearch--{{theme_settings.quickSearch-desktop-style}}{{/if}}{{/if}}\" data-prevent-quick-search-close>\r\n    {{{ snippet \"forms_search\"}}}\r\n    <form class=\"form\" action=\"{{urls.search}}\">\r\n        <fieldset class=\"form-fieldset\">\r\n            <div class=\"form-field\">\r\n                <label class=\"is-srOnly\" for=\"search_query{{#if style '===' 'mobileOnly'}}_mobile{{/if}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                <div class=\"form-prefixPostfix wrap\">\r\n                    <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query{{#if style '===' 'mobileOnly'}}_mobile{{/if}}\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                    <input type=\"submit\" class=\"button button--primary form-prefixPostfix-button--postfix\" value=\"{{lang 'common.search'}}\" />\r\n                </div>\r\n            </div>\r\n        </fieldset>\r\n    </form>\r\n</div>\r\n"
}