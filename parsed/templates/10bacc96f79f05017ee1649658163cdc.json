  "components/emthemes-modez/brand/carousel": "\r\n<article class=\"emthemesModez-brandCarousel-container\" data-section-type=\"brands-logo\">\r\n    {{#unless hide_heading}}\r\n    <div class=\"footer-info-heading\">{{lang 'footer.brands'}}</div>\r\n    {{/unless}}\r\n    <div class=\"emthemesModez-brandCarousel\" data-slick='{\r\n        \"dots\": false,\r\n        \"infinite\": false,\r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 2,\r\n        \"slidesToScroll\": 2,\r\n        \"responsive\": [\r\n            {\r\n                \"breakpoint\": 1260,\r\n                \"settings\": {\r\n                    \"slidesToScroll\": 2,\r\n                    \"slidesToShow\": 5\r\n                }\r\n            },\r\n            {\r\n                \"breakpoint\": 800,\r\n                \"settings\": {\r\n                    \"slidesToScroll\": 2,\r\n                    \"slidesToShow\": 5\r\n                }\r\n            },\r\n            {\r\n                \"breakpoint\": 550,\r\n                \"settings\": {\r\n                    \"slidesToScroll\": 2,\r\n                    \"slidesToShow\": 3\r\n                }\r\n            }\r\n        ]\r\n    }'>\r\n        {{#if type '==' 'static'}}\r\n            {{#if (lang 'emthemesmodez.brands.image1')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image1'}}\"><img src=\"{{lang 'emthemesmodez.brands.image1'}}\" alt=\"{{lang 'emthemesmodez.brands.title1'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image2')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image2'}}\"><img src=\"{{lang 'emthemesmodez.brands.image2'}}\" alt=\"{{lang 'emthemesmodez.brands.title2'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image3')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image3'}}\"><img src=\"{{lang 'emthemesmodez.brands.image3'}}\" alt=\"{{lang 'emthemesmodez.brands.title3'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image4')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image4'}}\"><img src=\"{{lang 'emthemesmodez.brands.image4'}}\" alt=\"{{lang 'emthemesmodez.brands.title4'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image5')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image5'}}\"><img src=\"{{lang 'emthemesmodez.brands.image5'}}\" alt=\"{{lang 'emthemesmodez.brands.title5'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image6')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image6'}}\"><img src=\"{{lang 'emthemesmodez.brands.image6'}}\" alt=\"{{lang 'emthemesmodez.brands.title6'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image7')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image7'}}\"><img src=\"{{lang 'emthemesmodez.brands.image7'}}\" alt=\"{{lang 'emthemesmodez.brands.title7'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image8')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image8'}}\"><img src=\"{{lang 'emthemesmodez.brands.image8'}}\" alt=\"{{lang 'emthemesmodez.brands.title8'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image9')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image9'}}\"><img src=\"{{lang 'emthemesmodez.brands.image9'}}\" alt=\"{{lang 'emthemesmodez.brands.title9'}}\" /></a></div>\r\n            {{/if}}\r\n            {{#if (lang 'emthemesmodez.brands.image10')}}\r\n            <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{lang 'emthemesmodez.brands.image10'}}\"><img src=\"{{lang 'emthemesmodez.brands.image10'}}\" alt=\"{{lang 'emthemesmodez.brands.title10'}}\" /></a></div>\r\n            {{/if}}\r\n        {{else}}\r\n            {{#each shop_by_brand}}\r\n                <div class=\"emthemesModez-brandCarousel-slide\"><a href=\"{{url}}\">{{name}}</a></div>\r\n            {{/each}}\r\n        {{/if}}\r\n    </div>\r\n    {{#unless hide_view_all}}\r\n    <p><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></p>\r\n    {{/unless}}\r\n</article>\r\n"
