{
  "components/papa-supermarket/also-bought/also-bought": "<div class=\"productView-alsoBought productView-alsoBought--{{theme_settings.alsobought_position}} u-hiddenVisually\" data-also-bought>\r\n    <div class=\"productView-alsoBought-heading\">{{lang 'also_bought.heading'}}</div>\r\n    <div class=\"productView-alsoBought-thumbnails\" data-thumbnails></div>\r\n    <div class=\"productView-alsoBought-buttons\">\r\n        <button class=\"button button--small button--primary\" data-add-all>{{lang 'also_bought.add_all'}}</button>\r\n        <button class=\"button button--small button--primary\" data-add-to-cart>{{lang 'also_bought.add_selected_to_cart'}}</button>\r\n    </div>\r\n    <div class=\"productView-alsoBought-list\">\r\n        {{#if theme_settings.alsobought_product '==' 'related'}}\r\n            {{#each product.related_products}}\r\n                <div class=\"productView-alsoBought-item\" data-product-id=\"{{id}}\"></div>\r\n                {{inject 'enableAlsoBought' true}}\r\n            {{/each}}\r\n        {{else if theme_settings.alsobought_product '==' 'similar'}}\r\n            {{#each product.similar_by_views}}\r\n                <div class=\"productView-alsoBought-item\" data-product-id=\"{{id}}\"></div>\r\n                {{inject 'enableAlsoBought' true}}\r\n            {{/each}}\r\n        {{else}}\r\n            {{#each product.custom_fields}}\r\n                {{#if name '===' '__alsobought'}}\r\n                    <div class=\"productView-alsoBought-item\" data-product-id=\"{{value}}\"></div>\r\n                    {{inject 'enableAlsoBought' true}}\r\n                {{/if}}\r\n            {{/each}}\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n"
}