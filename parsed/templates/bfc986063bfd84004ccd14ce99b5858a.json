{
  "components/emthemes-modez/banners/one": "<div class=\"emthemesModez-bannersContainer \r\n\t\t\temthemesModez-bannersContainer--one \r\n\t\t\temthemesModez-bannersContainer--one-{{index}} \r\n\t\t\t{{#if style}}emthemesModez-bannersContainer--one-{{style}}{{/if}}\">\r\n\r\n\t{{> components/emthemes-modez/banners/banner\r\n\t\turl=(lang (concat (concat \"emthemesmodez.banners.one_\" index) \".banner.url\"))\r\n\t\timage=(lang (concat (concat \"emthemesmodez.banners.one_\" index) \".banner.image\"))\r\n\t\ttitle=(lang (concat (concat \"emthemesmodez.banners.one_\" index) \".banner.title\"))\r\n\t\ttext=(lang (concat (concat \"emthemesmodez.banners.one_\" index) \".banner.text\"))\r\n\t\tbutton=(lang (concat (concat \"emthemesmodez.banners.one_\" index) \".banner.button\"))\r\n\t\tstyle=style\r\n\t}}\r\n\r\n</div>\r\n",
  "components/emthemes-modez/banners/banner": "\r\n<div class=\"emthemesModez-banner\">\r\n\t<div class=\"emthemesModez-banner-container\">\r\n\t\t<figure class=\"emthemesModez-banner-figure\">\r\n\t\t\t{{#if image '!=' ' '}}\r\n\t\t\t<a href=\"{{url}}\"><img class=\"emthemesModez-banner-image\" src=\"{{image}}\" alt=\"{{title}}\" title=\"{{title}}\" /></a>\r\n\t\t\t{{/if}}\r\n\t\t</figure>\r\n\r\n\r\n\t\t<figcaption class=\"emthemesModez-banner-figcaption {{#if title '==' ' '}}{{#if text '==' ' '}}{{#if button '==' ' '}}emthemesModez-banner-figcaption--empty{{/if}}{{/if}}{{/if}}\">\r\n\t\t\t<div class=\"emthemesModez-banner-figcaption-body\">\r\n\t\t\t\t{{#if title '!=' ' '}}\r\n\t\t\t\t<div class=\"emthemesModez-banner-figcaption-title\">{{title}}</div>\r\n\t\t\t\t{{/if}}\r\n\r\n\t\t\t\t{{#if text '!=' ' '}}\r\n\t\t\t\t<div class=\"emthemesModez-banner-figcaption-text\">{{text}}</div>\r\n\t\t\t\t{{/if}}\r\n\r\n\t\t\t\t{{#if button '!=' ' '}}\r\n\t\t\t\t<a href=\"{{url}}\" class=\"button button--primary button--small emthemesModez-banner-figcaption-button\">{{button}}</a>\r\n\t\t\t\t{{/if}}\r\n\t\t\t</div>\r\n\t\t</figcaption>\r\n\t</div>\r\n</div>\r\n"
}