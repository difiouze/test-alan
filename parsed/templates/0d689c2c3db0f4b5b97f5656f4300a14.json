  "components/common/navigation-page-list": "{{#if children}}\r\n<a class=\"navPages-action has-subMenu\" href=\"{{url}}\" \r\n    data-instantload\r\n    data-collapsible=\"navPages-page-{{@index}}\"\r\n    data-collapsible-disabled-breakpoint=\"medium\"\r\n    data-collapsible-disabled-state=\"open\"\r\n    data-collapsible-enabled-state=\"closed\">\r\n    {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n</a>\r\n<div class=\"navPage-subMenu navPage-subMenu--standard\" id=\"navPages-page-{{@index}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n        <li class=\"navPage-subMenu-item\">\r\n            <a class=\"navPage-subMenu-action navPages-action\" data-instantload href=\"{{url}}\">{{name}}</a>\r\n        </li>\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a\r\n                        class=\"navPage-subMenu-action navPages-action has-subMenu\"\r\n                        href=\"{{url}}\"\r\n                        data-instantload\r\n                        data-collapsible=\"navPages-page-{{../../index}}-{{@index}}\"\r\n                        data-collapsible-disabled-breakpoint=\"medium\"\r\n                        data-collapsible-disabled-state=\"open\"\r\n                        data-collapsible-enabled-state=\"closed\">\r\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-page-{{../../index}}-{{@index}}\">\r\n                        <li class=\"navPage-childList-item\">\r\n                            <a class=\"navPage-childList-action navPages-action\" data-instantload href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n       