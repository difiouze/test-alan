{
  "components/search/sidebar": "<nav>\r\n    {{#if product_results.faceted_search_enabled}}\r\n        {{> components/products/filter \r\n            sort=pagination.product_results.sort \r\n            pager=pagination.product_results\r\n            list=product_results\r\n            products_per_page=theme_settings.searchpage_products_per_page\r\n        }}\r\n        {{> components/faceted-search/index product_results}}\r\n    {{else}}\r\n        {{> components/products/filter \r\n            sort=pagination.product_results.sort \r\n            pager=pagination.product_results\r\n            list=product_results\r\n            products_per_page=theme_settings.searchpage_products_per_page\r\n        }}\r\n    {{/if}}\r\n</nav>\r\n",
  "components/products/filter": "<div class=\"emthemesModez-productsFilter {{#or category.faceted_search_enabled brand.faceted_search_enabled product_results.faceted_search_enabled}}emthemesModez-productsFilter--faceted{{/or}}\">\r\n    <a href=\"#emthemesModez-productsFilter-form\" role=\"button\" class=\"button emthemesModez-productsFilter-toggle\" aria-label=\"Options\" data-collapsible><i class=\"fa fa-cog\" aria-hidden=\"true\"></i></a>\r\n    <form class=\"actionBar\" id=\"emthemesModez-productsFilter-form\" method=\"get\" data-sort-by>\r\n        {{> components/category/sort-box sort=sort}}\r\n        {{> components/category/products-per-page products_per_page=products_per_page}}\r\n        {{> components/category/grid-list}}\r\n    </form>\r\n</div>\r\n",
  "components/faceted-search/index": "<div id=\"facetedSearch\" class=\"facetedSearch sidebarBlock\">\r\n    {{> components/faceted-search/selected-facets this.selected}}\r\n\r\n    <a href=\"#facetedSearch-navList\" role=\"button\" class=\"facetedSearch-toggle toggleLink\" data-collapsible>\r\n        <span class=\"facetedSearch-toggle-text\">\r\n            {{#if facets.length '>' 2}}\r\n                {{lang 'search.faceted.browse-by'}} {{ join (pluck facets 'title') ', ' limit=2 }} &amp; {{ toLowerCase (lang 'search.faceted.more') }}\r\n            {{/if}}\r\n\r\n            {{#if facets.length '<=' 2}}\r\n                {{lang 'search.faceted.browse-by'}} {{ join (pluck facets 'title') ', ' lastSeparator=' & ' }}\r\n            {{/if}}\r\n        </span>\r\n\r\n        <span class=\"facetedSearch-toggle-indicator\">\r\n            <span class=\"toggleLink-text toggleLink-text--on\">\r\n                {{lang 'search.faceted.hide-filters'}}\r\n\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-keyboard-arrow-up\"/></svg>\r\n                </i>\r\n            </span>\r\n\r\n            <span class=\"toggleLink-text toggleLink-text--off\">\r\n                {{lang 'search.faceted.show-filters'}}\r\n\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg><use xlink:href=\"#icon-keyboard-arrow-down\"/></svg>\r\n                </i>\r\n            </span>\r\n        </span>\r\n    </a>\r\n\r\n    {{#any facets visible=true}}\r\n        <div\r\n            id=\"facetedSearch-navList\"\r\n            class=\"facetedSearch-navList blocker-container\">\r\n            <div class=\"accordion accordion--navList\">\r\n                {{#each facets}}\r\n                    {{#if visible}}\r\n                        {{{dynamicComponent 'components/faceted-search/facets'}}}\r\n                    {{/if}}\r\n                {{/each}}\r\n            </div>\r\n\r\n            <div class=\"blocker\" style=\"display: none;\"></div>\r\n        </div>\r\n    {{/any}}\r\n</div>\r\n",
  "components/category/sort-box": "<fieldset class=\"form-fieldset actionBar-section actionBar-section--sort\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\r\n        <select class=\"form-select\" name=\"sort\" id=\"sort\">\r\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\r\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\r\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\r\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\r\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\r\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\r\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\r\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\r\n            {{#if result_count}}\r\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\r\n            {{/if}}\r\n        </select>\r\n    </div>\r\n</fieldset>\r\n\r\n",
  "components/category/products-per-page": "<fieldset class=\"form-fieldset actionBar-section actionBar-section--productsPerPage\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"limit\">{{lang 'common.sorter.products_per_page'}}:</label>\r\n        <select class=\"form-select\" name=\"limit\" id=\"limit\">\r\n            <option value=\"8\" {{#if products_per_page '==' 8}}selected{{/if}}>8</option>\r\n            <option value=\"12\" {{#if products_per_page '==' 12}}selected{{/if}}>12</option>\r\n            <option value=\"16\" {{#if products_per_page '==' 16}}selected{{/if}}>16</option>\r\n            <option value=\"20\" {{#if products_per_page '==' 20}}selected{{/if}}>20</option>\r\n            <option value=\"40\" {{#if products_per_page '==' 40}}selected{{/if}}>40</option>\r\n            <option value=\"100\" {{#if products_per_page '==' 100}}selected{{/if}}>100</option>\r\n        </select>\r\n    </div>\r\n</fieldset>\r\n\r\n",
  "components/category/grid-list": "<fieldset class=\"form-fieldset actionBar-section actionBar-section--mode\">\r\n    <div class=\"form-field\">\r\n        <label class=\"form-label\" for=\"mode\">{{lang 'common.sorter.columns'}}:</label>\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"1\" id=\"mode_1{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_1{{id_suffix}}\"><span class=\"form-option-variant\" title=\"1\">1</span></label>\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"2\" id=\"mode_2{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid2'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_2{{id_suffix}}\"><span class=\"form-option-variant\" title=\"2\">2</span></label>\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"3\" id=\"mode_3{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid3'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_3{{id_suffix}}\"><span class=\"form-option-variant\" title=\"3\">3</span></label>\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"4\" id=\"mode_4{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_4{{id_suffix}}\"><span class=\"form-option-variant\" title=\"4\">4</span></label>\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"6\" id=\"mode_6{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid6'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_6{{id_suffix}}\"><span class=\"form-option-variant\" title=\"6\">6</span></label>\r\n        {{#if theme_settings.show_bulk_order_mode}}\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"bo\" id=\"mode_bo{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'listbo'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_bo{{id_suffix}}\"><span class=\"form-option-variant\" title=\"{{lang 'bulk_order.icon_title'}}\"><i class=\"fa fa-th-list\"></i></span></label>\r\n        {{/if}}\r\n\r\n        <!--\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"grid\" id=\"mode_grid{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'grid'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_grid{{id_suffix}}\" aria-label=\"Grid\"><span class=\"form-option-variant\" title=\"Grid\"><i class=\"fa fa-th-large\" aria-hidden=\"true\"></i></span></label>\r\n        <input class=\"form-radio\" type=\"radio\" name=\"mode\" value=\"list\" id=\"mode_list{{id_suffix}}\" {{#if theme_settings.product_list_display_mode '===' 'list'}} checked{{/if}}>\r\n        <label class=\"form-option\" for=\"mode_list{{id_suffix}}\" aria-label=\"List\"><span class=\"form-option-variant\" title=\"List\"><i class=\"fa fa-th-list\" aria-hidden=\"true\"></i></span></label>\r\n        -->\r\n    </div>\r\n</fieldset>\r\n",
  "components/faceted-search/selected-facets": "<div class=\"facetedSearch-refineFilters sidebarBlock\">\r\n    <div class=\"sidebarBlock-heading\">\r\n        {{lang 'search.faceted.selected.title'}}\r\n    </div>\r\n\r\n    {{#if this.items}}\r\n        <ul class=\"inlineList inlineList--labels\">\r\n            {{#each this.items}}\r\n                <li>\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"facetLabel\"\r\n                        rel=\"nofollow\"\r\n                        data-faceted-search-facet>\r\n                        {{#if facet '===' 'rating'}}\r\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\r\n                        {{else}}\r\n                            {{{sanitize title}}}\r\n                        {{/if}}\r\n\r\n                        <svg class=\"icon\">\r\n                            <use xlink:href=\"#icon-close\" />\r\n                        </svg>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        {{lang 'search.faceted.selected.no-filters'}}\r\n    {{/if}}\r\n\r\n    {{#if remove_all_url}}\r\n        <a href=\"{{ remove_all_url }}\" data-faceted-search-facet>\r\n            {{lang 'search.faceted.selected.clear-all'}}\r\n        </a>\r\n    {{/if}}\r\n</div>\r\n",
  "components/faceted-search/facets/hierarchy-children": "<ul class=\"navList navList--inner\">\r\n    {{#each this}}\r\n        <li class=\"navList-item\">\r\n            <a\r\n            href=\"{{url}}\"\r\n            class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\r\n            rel=\"nofollow\"\r\n            data-id=\"{{ id }}\"\r\n            data-faceted-search-facet>\r\n                {{{sanitize title}}}\r\n                {{#if ../show_product_counts}}\r\n                    {{#if count}}\r\n                        <span>({{count}})</span>\r\n                    {{/if}}\r\n                {{/if}}\r\n            </a>\r\n            {{#if children}}\r\n                {{> components/faceted-search/facets/hierarchy-children children}}\r\n            {{/if}}\r\n        </li>\r\n    {{/each}}\r\n</ul>\r\n",
  "components/faceted-search/facets/hierarchy": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\">\r\n        <div class=\"accordion-title\">\r\n            {{#startsWith '__' title}}\r\n                {{last (split title '__')}}\r\n            {{else}}\r\n                {{ title }}\r\n            {{/startsWith}}\r\n        </div>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{dashcase facet}}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <ul id=\"facetedSearch-navList--{{dashcase facet}}\" class=\"navList\" data-facet=\"{{facet}}\" data-has-more-results=\"{{ has_more_results }}\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}} {{#if has_sub_categories }} has-children {{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-id=\"{{ id }}\"\r\n                        data-faceted-search-facet>\r\n                        {{{sanitize title}}}\r\n                        {{#if ../show_product_counts}}\r\n                            {{#if count}}\r\n                                <span>({{ count }})</span>\r\n                            {{/if}}\r\n                        {{/if}}\r\n                    </a>\r\n                    {{#if children}}\r\n                        {{> components/faceted-search/facets/hierarchy-children children}}\r\n                    {{/if}}\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n\r\n        {{#if show_more_toggle}}\r\n            <a href=\"#facetedSearch-navList--{{dashcase facet}}\" role=\"button\" class=\"toggleLink\">\r\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\r\n            </a>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/faceted-search/facets/multi": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\">\r\n        <div class=\"accordion-title\">\r\n            {{#startsWith '__' title}}\r\n                {{last (split title '__')}}\r\n            {{else}}\r\n                {{ title }}\r\n            {{/startsWith}}\r\n        </div>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{dashcase facet}}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <ul id=\"facetedSearch-navList--{{dashcase facet}}\" data-facet=\"{{facet}}\" class=\"navList\" data-has-more-results=\"{{ has_more_results }}\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"navList-action navList-action--checkbox {{#if selected }} is-selected {{/if}}\"\r\n                        rel=\"nofollow\"\r\n                        data-faceted-search-facet>\r\n                        {{{sanitize title}}}\r\n                        {{#if ../show_product_counts}}\r\n                            <span>({{ count }})</span>\r\n                        {{/if}}\r\n\r\n                        <span class=\"navList-action-close\" aria-hidden=\"true\">\r\n                            <svg class=\"icon\">\r\n                                <use xlink:href=\"#icon-close\"/>\r\n                            </svg>\r\n                        </span>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n\r\n        {{#if show_more_toggle}}\r\n            <a href=\"#facetedSearch-navList--{{dashcase facet}}\" role=\"button\" class=\"toggleLink\">\r\n                <span class=\"toggleLink-text toggleLink-text--off\">{{lang 'search.faceted.show-more'}}</span>\r\n            </a>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/faceted-search/facets/range": "<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\r\n        <div class=\"accordion-title\">\r\n            {{ title }}\r\n        </div>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"accordion-content {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\r\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\r\n            {{#if this.sort}}\r\n                <input type=\"hidden\" name=\"sort\" value={{this.sort}}>\r\n            {{/if}}\r\n            <fieldset class=\"form-fieldset\">\r\n                <div class=\"form-minMaxRow\">\r\n                    <div class=\"form-field\">\r\n                        <input\r\n                            name=\"min_price\"\r\n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\r\n                            min=\"0\"\r\n                            class=\"form-input form-input--small\"\r\n                            required\r\n                            type=\"number\"\r\n                            value=\"{{ min_price }}\"\r\n                        />\r\n                    </div>\r\n\r\n                    <div class=\"form-field\">\r\n                        <input\r\n                            name=\"max_price\"\r\n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\r\n                            min=\"0\"\r\n                            class=\"form-input form-input--small\"\r\n                            required\r\n                            type=\"number\"\r\n                            value=\"{{ max_price }}\"\r\n                        />\r\n                    </div>\r\n\r\n                    <div class=\"form-field\">\r\n                        <button class=\"button button--small\" type=\"submit\">\r\n                            {{lang 'search.faceted.range.update'}}\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"form-inlineMessage\"></div>\r\n            </fieldset>\r\n        </form>\r\n    </div>\r\n</div>\r\n",
  "components/faceted-search/facets/rating": "{{#if items}}\r\n<div class=\"accordion-block\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink {{#unless ../start_collapsed }} is-open {{/unless}}\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--{{dashcase facet }}\">\r\n        <div class=\"accordion-title\">\r\n            {{ title }}\r\n        </div>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\r\n                <use xlink:href=\"#icon-add\" />\r\n            </svg>\r\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\r\n                <use xlink:href=\"#icon-remove\" />\r\n            </svg>\r\n        </div>\r\n    </div>\r\n\r\n    <div id=\"facetedSearch-content--{{dashcase facet }}\" class=\"accordion-content facetedSearch-content--{{dashcase facet}} {{#unless ../start_collapsed }} is-open {{/unless}}\">\r\n        <ul class=\"navList\">\r\n            {{#each items}}\r\n                <li class=\"navList-item\">\r\n                    <a href=\"{{ url }}\"\r\n                       class=\"navList-action {{#if selected }} is-selected {{/if}}\"\r\n                       rel=\"nofollow\"\r\n                       data-faceted-search-facet>\r\n                        <span class=\"rating--small\">\r\n                            {{> components/products/ratings rating=title}}\r\n                        </span>\r\n                        {{lang 'search.faceted.rating.and-up'}}\r\n                        {{#if ../show_product_counts}}\r\n                            <span>({{ count }})</span>\r\n                        {{/if}}\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </div>\r\n</div>\r\n{{/if}}\r\n",
  "components/products/ratings": "{{#for 1 5}}\r\n    {{#if ../this.rating '>=' $index}}\r\n        <span class=\"icon icon--ratingFull\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-star\" />\r\n            </svg>\r\n        </span>\r\n    {{else}}\r\n        <span class=\"icon icon--ratingEmpty\">\r\n            <svg>\r\n                <use xlink:href=\"#icon-star\" />\r\n            </svg>\r\n        </span>\r\n    {{/if}}\r\n{{/for}}\r\n{{{snippet 'product_rating'}}}\r\n"
}